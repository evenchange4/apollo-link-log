{
  "name": "apollo-link-log",
  "version": "1.1.0",
  "typings": "./index.d.ts",
  "main": "lib/index.js",
  "module": "es/index.js",
  "sideEffects": false,
  "files": [
    "es",
    "lib",
    "index.d.ts"
  ],
  "scripts": {
    "build": "hsu-scripts build src",
    "build-storybook": "build-storybook",
    "test": "NODE_ENV='test' jest --coverage",
    "test:watch": "npm run test -- --watch",
    "start": "start-storybook -p 6006",
    "eslint": "eslint ./",
    "flow": "flow",
    "flow-coverage": "hsu-scripts flow --threshold 52",
    "precommit": "lint-staged",
    "format": "prettier --write '**/*.{js,json,md,css,yaml,yml}' '*.{js,json,md,css,yaml,yml}'",
    "changelog": "conventional-changelog --infile ./CHANGELOG.md --same-file --release-count 0 --output-unreleased"
  },
  "dependencies": {
    "chalk": "^2.4.1"
  },
  "devDependencies": {
    "apollo-link": "1.2.6",
    "ast-pretty-print": "2.0.1",
    "codecov": "3.1.0",
    "conventional-changelog-cli": "2.0.11",
    "eslint": "5.12.0",
    "flow-bin": "0.90.0",
    "graphql": "14.1.0",
    "graphql-tag": "2.10.0",
    "hsu-scripts": "1.0.2",
    "husky": "1.3.1",
    "jest": "24.0.0-alpha.12",
    "lint-staged": "8.1.0",
    "prettier": "1.15.3"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.js",
      "!src/**/*.test.js"
    ],
    "testPathIgnorePatterns": [
      "<rootDir>/node_modules/",
      "<rootDir>/lib/",
      "<rootDir>/es/"
    ],
    "resetMocks": true,
    "resetModules": true
  },
  "babel": {
    "presets": [
      "./node_modules/hsu-scripts/babel.js"
    ]
  },
  "eslintConfig": {
    "extends": [
      "./node_modules/hsu-scripts/eslint.js"
    ]
  },
  "lint-staged": {
    "*.{js,json,md,css,yaml,yml}": [
      "prettier --write",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "description": "Log side effect for Apollo Link.",
  "author": "Michael Hsu <evenchange4@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/evenchange4/apollo-link-log/issues",
    "email": "evenchange4@gmail.com"
  },
  "homepage": "https://github.com/evenchange4/apollo-link-log#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/evenchange4/apollo-link-log.git"
  }
}
